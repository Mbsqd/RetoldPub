{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RetoldDbContext": "Host=DB_HOST;Port=DB_PORT;Database=DB_NAME;Username=DB_USER;Password=DB_PASS;"
  },
  "Authentication": {
    "JwtOptions": {
      "SecretKey": "paste-the-generated-string-here.",
      "ExpiresMinutes": "60",
      "Audience": "https://frontend:3000",
      "Issuer": "https://backend:8080"
    }
  }
}
